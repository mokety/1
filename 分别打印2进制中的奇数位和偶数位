#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
void print(int a)
{

	int i = 0;

	for (i = 30; i >=0; i -= 2)
	{	
		printf("%d",(a>>i)&1);
		
	}
	printf("\n");
	for (i = 31; i >= 0; i -= 2)
	{
		printf("%d", (a >> i) & 1);

	}

}
int main()
{
	int a = 0;
	scanf("%d", &a);
	print(a);
	printf("\n");
	

	return 0;
}
